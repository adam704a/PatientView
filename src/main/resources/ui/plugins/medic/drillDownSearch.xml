<?xml version="1.0" encoding="UTF-8"?>
<panel border="false" columns="4" gap="5" name="searchPanel" weightx="1">
	<button action="back()" alignment="left" halign="left" name="medicBackButton" icon="/icons/left.png" />
	<button action="refresh()" alignment="left" halign="left" name="medicRefreshButton" icon="/icons/refresh.png" />
	<button action="forward()" alignment="left" halign="left" name="medicForwardButton" icon="/icons/right.png" />
	<panel name="breadcrumbPanel" gap="10" halign="left" left="10" valign="center" top="5" weightx="8" />
	<label text="Search:" colspan="2" icon="/icons/search.png" halign="center" />
	<textfield name="searchBar" colspan="2" action="search(searchBar.text)" columns="1" weightx="8" />
	<panel border="true" colspan="4" name="organizerPanel" weightx="1" halign="center" valign="center" />
	<label name="searchDescriptorLabel" text="" colspan="4" halign="center" />
</panel>